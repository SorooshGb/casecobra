generator client {
  provider = "prisma-client-js"
  output   = "../src/db/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Configuration {
  id              String  @id @default(cuid())
  width           Int
  height          Int
  imgUrl          String
  croppedImageUrl String?
}
